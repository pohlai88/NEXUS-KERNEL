{
  "name": "@aibos/kernel",
  "version": "1.1.0",
  "description": "AIBOS Kernel - The Business Constitution (L0 SSOT)",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "module": "./dist/index.js",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ],
      "index": [
        "./dist/index.d.ts"
      ]
    }
  },
  "browser": {
    "./dist/index.js": "./dist/index.js"
  },
  "react-native": {
    "./dist/index.js": "./dist/index.js"
  },
  "exports": {
    ".": {
      "@aibos/kernel/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.js"
    },
    "./values/lazy": {
      "@aibos/kernel/source": "./src/values.lazy.ts",
      "types": "./dist/values.lazy.d.ts",
      "import": {
        "types": "./dist/values.lazy.d.ts",
        "default": "./dist/values.lazy.js"
      },
      "require": {
        "types": "./dist/values.lazy.d.cts",
        "default": "./dist/values.lazy.cjs"
      },
      "default": "./dist/values.lazy.js"
    },
    "./validation": {
      "@aibos/kernel/source": "./src/kernel.validation.ts",
      "types": "./dist/kernel.validation.d.ts",
      "import": {
        "types": "./dist/kernel.validation.d.ts",
        "default": "./dist/kernel.validation.js"
      },
      "require": {
        "types": "./dist/kernel.validation.d.cts",
        "default": "./dist/kernel.validation.cjs"
      },
      "default": "./dist/kernel.validation.js"
    },
    "./cache": {
      "@aibos/kernel/source": "./src/kernel.validation.cache.ts",
      "types": "./dist/kernel.validation.cache.d.ts",
      "import": {
        "types": "./dist/kernel.validation.cache.d.ts",
        "default": "./dist/kernel.validation.cache.js"
      },
      "require": {
        "types": "./dist/kernel.validation.cache.d.cts",
        "default": "./dist/kernel.validation.cache.cjs"
      },
      "default": "./dist/kernel.validation.cache.js"
    },
    "./cache/supabase": {
      "@aibos/kernel/source": "./src/kernel.validation.cache.supabase.ts",
      "types": "./dist/kernel.validation.cache.supabase.d.ts",
      "import": {
        "types": "./dist/kernel.validation.cache.supabase.d.ts",
        "default": "./dist/kernel.validation.cache.supabase.js"
      },
      "require": {
        "types": "./dist/kernel.validation.cache.supabase.d.cts",
        "default": "./dist/kernel.validation.cache.supabase.cjs"
      },
      "default": "./dist/kernel.validation.cache.supabase.js"
    },
    "./supabase": {
      "@aibos/kernel/source": "./src/supabase/index.ts",
      "types": "./dist/supabase/index.d.ts",
      "import": {
        "types": "./dist/supabase/index.d.ts",
        "default": "./dist/supabase/index.js"
      },
      "require": {
        "types": "./dist/supabase/index.d.cts",
        "default": "./dist/supabase/index.cjs"
      },
      "default": "./dist/supabase/index.js"
    },
    "./monitoring": {
      "@aibos/kernel/source": "./src/monitoring/performance.ts",
      "types": "./dist/monitoring/performance.d.ts",
      "import": {
        "types": "./dist/monitoring/performance.d.ts",
        "default": "./dist/monitoring/performance.js"
      },
      "require": {
        "types": "./dist/monitoring/performance.d.cts",
        "default": "./dist/monitoring/performance.cjs"
      },
      "default": "./dist/monitoring/performance.js"
    },
    "./nextjs": {
      "@aibos/kernel/source": "./src/nextjs/index.ts",
      "types": {
        "import": "./dist/nextjs/index.d.ts",
        "require": "./dist/nextjs/index.d.cts",
        "default": "./dist/nextjs/index.d.ts"
      },
      "import": {
        "types": "./dist/nextjs/index.d.ts",
        "default": "./dist/nextjs/index.js"
      },
      "require": {
        "types": "./dist/nextjs/index.d.cts",
        "default": "./dist/nextjs/index.cjs"
      },
      "default": "./dist/nextjs/index.js"
    },
    "./package.json": "./package.json",
    "./v1": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "registry.snapshot.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "keywords": [
    "kernel",
    "aibos",
    "nexus",
    "ssot",
    "governance",
    "l0"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pohlai88/NEXUS-KERNEL.git"
  },
  "dependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.3",
    "@types/node": "^20.19.27",
    "@vitest/coverage-v8": "^4.0.16",
    "next": "^16.1.1",
    "semantic-release": "^25.0.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.16"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "build:legacy": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "dev": "pnpm run build && node scripts/dev-server.js",
    "dev:watch": "pnpm run build:watch",
    "dev:server": "node scripts/dev-server.js",
    "generate": "tsx scripts/generate-kernel.ts",
    "generate:watch": "tsx watch scripts/generate-kernel.ts",
    "release:kernel": "tsx scripts/release-kernel.ts",
    "changelog:generate": "tsx scripts/generate-changelog.ts",
    "validate:kernel": "pnpm generate && pnpm typecheck && pnpm test",
    "docs:generate": "typedoc",
    "docs:schema": "tsx scripts/generate-schema-docs.ts",
    "docs:status": "tsx scripts/generate-status-report.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "release": "semantic-release",
    "perf:profile": "tsx scripts/performance-profile.ts",
    "perf:test": "vitest run -- performance.test.ts",
    "analyze:bundle": "tsx scripts/analyze-bundle.ts",
    "build:prod": "NODE_ENV=production pnpm build",
    "build:analyze": "pnpm build && pnpm analyze:bundle",
    "generate:migration": "tsx scripts/generate-supabase-migration.ts",
    "perf:dashboard": "tsx scripts/performance-dashboard.ts",
    "update:repo": "tsx scripts/update-repo.ts",
    "update": "pnpm update:repo",
    "audit:docs": "tsx scripts/audit-docs-quality.ts"
  }
}